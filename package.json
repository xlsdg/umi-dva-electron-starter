{
  "author": "xLsDg",
  "build": {
    "appId": "your.app.id",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "category": "your.app.category.type"
    },
    "productName": "ElectronReact"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "^4.3.5",
    "classnames": "^2.2.6",
    "dva": "next",
    "fast-deep-equal": "^3.1.3",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "path-to-regexp": "^6.1.0",
    "prop-types": "^15.7.2",
    "re-reselect": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reselect": "^4.0.0",
    "source-map-support": "^0.5.19",
    "umi-request": "^1.3.5"
  },
  "description": "Get started with Umi.js and Electron.",
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@umijs/plugin-antd": "^0.6.0",
    "@umijs/plugin-dva": "^0.7.2",
    "@umijs/plugin-locale": "^0.8.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "electron-webpack": "^2.8.2",
    "eslint": "^7.3.1",
    "eslint-config-alloy": "^3.7.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "umi": "^3.2.6",
    "webpack": "^4.43.0"
  },
  "electronWebpack": {
    "renderer": {
      "sourceDirectory": null
    }
  },
  "license": "MIT",
  "name": "umi-dva-electron-starter",
  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "electron-webpack",
    "build:renderer": "cross-env APP_ROOT=src/renderer umi build",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "start": "concurrently \"npm run start:renderer\" \"npm run start:main\"",
    "start:main": "electron-webpack dev",
    "start:renderer": "cross-env APP_ROOT=src/renderer umi dev"
  },
  "version": "1.0.0"
}
